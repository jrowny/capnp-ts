{
  "author": "jdiaz5513",
  "bugs": {
    "url": "https://github.com/jdiaz5513/capnp-ts/issues"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "tslib": "^2.2.0"
  },
  "description": "Strongly typed Cap'n Proto implementation for the browser and Node.js using TypeScript",
  "engines": {
    "node": ">=10.4.0"
  },
  "homepage": "https://github.com/jdiaz5513/capnp-ts#readme",
  "keywords": [
    "capnp",
    "rpc",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./src/serialization": {
      "types": "./src/serialization/index.d.ts",
      "import": "./src/serialization/index.js"
    },
    "./src/serialization/arena": {
      "types": "./src/serialization/arena/index.d.ts",
      "import": "./src/serialization/arena/index.js"
    },
    "./src/serialization/pointers": {
      "types": "./src/serialization/pointers/index.d.ts",
      "import": "./src/serialization/pointers/index.js"
    },
    "./src/*.js": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "name": "capnp-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdiaz5513/capnp-ts.git"
  },
  "types": "./src/index.d.ts",
  "version": "0.7.0"
}
